<http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation rdfs:seeAlso ;
      lden:threshold "0.0"^^xsd:double .

<http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_source>
      a       lden:SourceDataset ;
      rdfs:label "source_a" ;
      lden:endPoint <http://eculture2.cs.vu.nl:8890/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?a rdf:type swc:AcademicEvent" ;
      lden:variable "a" .

<http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_acceptance>
      a       lden:Acceptance ;
      lden:file <links.nt> ;
      lden:relation rdfs:seeAlso ;
      lden:threshold "0.7"^^xsd:double .

<http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_metric>
      a       lden:Metric ;
      lden:expression "average(jaccard(b.dcterms:partOf,a.rdfs:label|0.5))" .

<http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_target>
      a       lden:TargetDataset ;
      rdfs:label "source_b" ;
      lden:endPoint <http://dblp.l3s.de/d2r/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?b rdf:type foaf:Document" ;
      lden:variable "b" .

<http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec>
      a       lden:LimesSpecs ;
      dcterms:created "2011" ;
      dcterms:creator "LATC" ;
      dcterms:license <https://github.com/LATC/sandbox/blob/master/linksets/README> ;
      dcterms:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dcterms:publisher "AKSW" ;
      dcterms:source <https://github.com/LATC/sandbox/tree/master/linksets> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^xsd:int ;
      lden:hasAcceptance <http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_acceptance> ;
      lden:hasMetric <http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_metric> ;
      lden:hasReview <http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_review> ;
      lden:hasSource <http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_source> ;
      lden:hasTarget <http://www.linklion.org/lden/eventseer-dblp_l3s-publications_spec_target> ;
      lden:outputFormat "ntriples" .
