lden:restaurants_spec_review
      a       lden:Review ;
      lden:file <reviewme.txt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.95"^^xsd:double .

lden:restaurants_spec
      a       lden:LimesSpecs ;
      dcterms:created "2011" ;
      dcterms:creator "AKSW" ;
      dcterms:license <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      dcterms:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dcterms:publisher "AKSW" ;
      dcterms:source <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "SIMPLE" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:restaurants_spec_acceptance ;
      lden:hasMetric lden:restaurants_spec_metric ;
      lden:hasReview lden:restaurants_spec_review ;
      lden:hasSource lden:restaurants_spec_source ;
      lden:hasTarget lden:restaurants_spec_target ;
      lden:outputFormat "N3" .

lden:restaurants_spec_target
      a       lden:TargetDataset ;
      rdfs:label "restaurant2" ;
      lden:endPoint <resources/Restaurants/restaurant2.nt> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property okkamrestaurant2:phone_number , okkamrestaurant2:has_address , okkamrestaurant2:name ;
      lden:restriction "?x rdf:type okkamrestaurant2:Restaurant" ;
      lden:variable "?y" .

lden:restaurants_spec_metric
      a       lden:Metric ;
      lden:expression "levenshtein(x.okkamrestaurant1:name, y.okkamrestaurant2:name)" .

lden:restaurants_spec_acceptance
      a       lden:Acceptance ;
      lden:file <accepted.txt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.98"^^xsd:double .

lden:restaurants_spec_source
      a       lden:SourceDataset ;
      rdfs:label "restaurant1" ;
      lden:endPoint <resources/Restaurants/restaurant1.nt> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property okkamrestaurant1:has_address , okkamrestaurant1:category , okkamrestaurant1:name , okkamrestaurant1:phone_number ;
      lden:restriction "?x rdf:type okkamrestaurant1:Restaurant" ;
      lden:variable "?x" .
