@prefix swc:     <http://data.semanticweb.org/ns/swc/ontology#> .

<www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^xsd:double .

<www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec>
      a       lden:LimesSpecs ;
      dcterms:created "2011" ;
      dcterms:creator "LATC" ;
      dcterms:license <https://github.com/LATC/sandbox/blob/master/linksets/README> ;
      dcterms:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dcterms:publisher "AKSW" ;
      dcterms:source <https://github.com/LATC/sandbox/tree/master/linksets> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^xsd:int ;
      lden:hasAcceptance <www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_acceptance> ;
      lden:hasMetric <www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_metric> ;
      lden:hasReview <www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_review> ;
      lden:hasSource <www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_source> ;
      lden:hasTarget <www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_target> ;
      lden:outputFormat "ntriples" .

<www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_target>
      a       lden:TargetDataset ;
      rdfs:label "linkedgeodata" ;
      lden:endPoint <http://linkedgeodata.org/sparql/> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?b a lgdo:PostOffice.  ?b wgs84:lat ?lat. ?b wgs84:long ?long." ;
      lden:variable "b" .

<www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_metric>
      a       lden:Metric ;
      lden:expression "average(wgs84(b.wgs84:lat/wgs84:long,a.wgs84:lat/wgs84:long|0.9),max(jaro(b.rdfs:label,a.geonames:name|0.9),jaro(b.rdfs:label,a.geonames:alternateName|0.9)))" .

<www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_source>
      a       lden:SourceDataset ;
      rdfs:label "geonames" ;
      lden:endPoint <http://lgd.aksw.org:8900/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?a geonames:featureCode geonames:PO.  ?a wgs84:lat ?lat. ?a wgs84:long ?long." ;
      lden:variable "a" .

<www.linklion.org/lden#/geonames-linkedgeodata-post_office_spec_acceptance>
      a       lden:Acceptance ;
      lden:file <1303320108589/post_office/verify.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.95"^^xsd:double .
