lden:drugbank-sider_spec_source
      a       lden:SourceDataset ;
      rdfs:label "sider" ;
      lden:endPoint <http://lgd.aksw.org:5678/sparql> ;
      lden:pageSize "500"^^xsd:int ;
      lden:property drugbank:brandName ;
      lden:restriction "?x rdf:type drugbank:drugs" ;
      lden:variable "?x" .

lden:drugbank-sider_spec
      a       lden:LimesSpecs ;
      dc:created "2011" ;
      dc:creator "AKSW" ;
      dc:license <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      dc:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dc:publisher "AKSW" ;
      dc:source <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "Simple" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:drugbank-sider_spec_acceptance ;
      lden:hasMetric lden:drugbank-sider_spec_metric ;
      lden:hasReview lden:drugbank-sider_spec_review ;
      lden:hasSource lden:drugbank-sider_spec_source ;
      lden:hasTarget lden:drugbank-sider_spec_target ;
      lden:outputFormat "TAB" .

lden:drugbank-sider_spec_metric
      a       lden:Metric ;
      lden:expression "Trigram(x.drugbank:brandName,y.sider:drugName)" .

lden:drugbank-sider_spec_acceptance
      a       lden:Acceptance ;
      lden:file <drugbank_sider_accepted.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "1.0"^^xsd:double .

lden:drugbank-sider_spec_target
      a       lden:TargetDataset ;
      rdfs:label "sider" ;
      lden:endPoint <http://lgd.aksw.org:5678/sparql> ;
      lden:pageSize "500"^^xsd:int ;
      lden:property sider:drugName ;
      lden:restriction "?y rdf:type sider:drugs" ;
      lden:variable "?y" .

lden:drugbank-sider_spec_review
      a       lden:Review ;
      lden:file <drugbank_sider_reviewme.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.99"^^xsd:double .
