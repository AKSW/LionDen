<www.linklion.org/lden#/bnb-dbpedia-novelists_spec_target>
      a       lden:TargetDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint <http://dbpedia.org/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?novel dbpediaowl:author ?b ." ;
      lden:variable "b" .

<www.linklion.org/lden#/bnb-dbpedia-novelists_spec_acceptance>
      a       lden:Acceptance ;
      lden:file <bnb-dbpedia-novelists.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.6"^^xsd:double .

<www.linklion.org/lden#/bnb-dbpedia-novelists_spec_source>
      a       lden:SourceDataset ;
      rdfs:label "bluk-bnb" ;
      lden:endPoint <http://bnb.data.bl.uk/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?a a foaf:Person ; bibliographic:has_created ?work ." ;
      lden:variable "a" .

<www.linklion.org/lden#/bnb-dbpedia-novelists_spec_metric>
      a       lden:Metric ;
      lden:expression "average(equality(b.dbpediaowl:author,a.bibliographic:has_created|0.65),jaroWinkler(b.foaf:name,a.foaf:name|0.65),equality(b.dbpediaowl:author,a.bibliographic:hasCreated|0.65))" .

<www.linklion.org/lden#/bnb-dbpedia-novelists_spec_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^xsd:double .

