@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix dc:      <http://purl.org/dc/terms/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix swpo:    <http://sw-portal.deri.org/ontologies/swportal#> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix mo:      <http://purl.org/ontology/mo/> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix linkedmdb:  <http://data.linkedmdb.org/resource/movie/> .
@prefix lden:    <http://www.linklion.org/lden/> .
@prefix dbpedia-prop:  <http://dbpedia.org/property/> .
@prefix yago:    <http://dbpedia.org/class/yago/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix v:       <http://www.w3.org/2006/vcard/ns#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .
@prefix dbpprop:  <http://dbpedia.org/property/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .

<http://www.linklion.org/lden/spec_census_source>
      a       lden:SourceDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint <http://dbpedia.org/sparql> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?a dbpedia-prop:wikiPageUsesTemplate <http://dbpedia.org/resource/Template:Infobox_Settlement>" ;
      lden:variable "a" .

<http://www.linklion.org/lden/spec_census_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<http://www.linklion.org/lden/spec_census_target>
      a       lden:TargetDataset ;
      rdfs:label "Census" ;
      lden:endPoint <http://www.rdfabout.com/sparql> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?b rdf:type usgov:County" ;
      lden:variable "b" .

<http://www.linklion.org/lden/spec_census_acceptance>
      a       lden:Acceptance ;
      lden:file <> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<http://www.linklion.org/lden/spec_census>
      a       lden:LimesSpecs ;
      dc:created "2011" ;
      dc:creator "DBPedia" ;
      dc:license <https://github.com/dbpedia/dbpedia-links/blob/master/README> ;
      dc:provenance "Data are obtained from https://github.com/dbpedia/dbpedia-links" ;
      dc:publisher "AKSW" ;
      dc:source <https://github.com/dbpedia/dbpedia-links> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <http://www.linklion.org/lden/spec_census_acceptance> ;
      lden:hasMetric <http://www.linklion.org/lden/spec_census_metric> ;
      lden:hasReview <http://www.linklion.org/lden/spec_census_review> ;
      lden:hasSource <http://www.linklion.org/lden/spec_census_source> ;
      lden:hasTarget <http://www.linklion.org/lden/spec_census_target> ;
      lden:outputFormat "" .

<http://www.linklion.org/lden/spec_census_metric>
      a       lden:Metric ;
      lden:expression "average(wgs84(b.wgs84:lat/wgs84:long,a.wgs84:lat/wgs84:long|0.9),max(jaro(b.rdfs:label,a.rdfs:label|0.9)))" .
