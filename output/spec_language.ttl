<www.linklion.org/lden#/spec_language_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation dbpedia-owl:spokenIn ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/spec_language_metric>
      a       lden:Metric ;
      lden:expression "max(jaro(b.rdfs:label,a.dbpedia-owl:spokenIn|0.90))" .

<www.linklion.org/lden#/spec_language_acceptance>
      a       lden:Acceptance ;
      lden:file <factbooklang_links_verify.nt> ;
      lden:relation dbpedia-owl:spokenIn ;
      lden:threshold "0.9"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/spec_language_target>
      a       lden:TargetDataset ;
      rdfs:label "factbook" ;
      lden:endPoint <http://www4.wiwiss.fu-berlin.de/factbook/sparql> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?b rdf:type factbook:Country" ;
      lden:variable "b" .

<www.linklion.org/lden#/spec_language>
      a       lden:LimesSpecs ;
      dc:created "2011" ;
      dc:creator "DBPedia" ;
      dc:license <https://github.com/dbpedia/dbpedia-links/blob/master/README> ;
      dc:provenance "Data are obtained from https://github.com/dbpedia/dbpedia-links" ;
      dc:publisher "AKSW" ;
      dc:source <https://github.com/dbpedia/dbpedia-links> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <www.linklion.org/lden#/spec_language_acceptance> ;
      lden:hasMetric <www.linklion.org/lden#/spec_language_metric> ;
      lden:hasReview <www.linklion.org/lden#/spec_language_review> ;
      lden:hasSource <www.linklion.org/lden#/spec_language_source> ;
      lden:hasTarget <www.linklion.org/lden#/spec_language_target> ;
      lden:outputFormat "alignment" .

<www.linklion.org/lden#/spec_language_source>
      a       lden:SourceDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint <http://dbpedia.org/sparql/> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?a rdf:type dbpedia-owl:Language" ;
      lden:variable "a" .
