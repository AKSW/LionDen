@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix swpo:    <http://sw-portal.deri.org/ontologies/swportal#> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix lden:    <http://www.linklion.org/lden/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .

<http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_target>
      a       lden:TargetDataset ;
      rdfs:label "linkedgeodata" ;
      lden:endPoint <http://linkedgeodata.org/sparql/> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "{?b a lgdo:Water}. ?b wgs84:geometry ?point." ;
      lden:variable "b" .

<http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_metric>
      a       lden:Metric ;
      lden:expression "average(wgs84(b.geo:geometry,a.geo:geometry|0.9),max(jaro(b.rdfs:label,a.rdfs:label|0.9)))" .

<http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_acceptance>
      a       lden:Acceptance ;
      lden:file <dbpedia-linkedgeodata-lake_verify.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.95"^^<http://www.w3.org/2001/XMLSchema#double> .

<http://www.linklion.org/lden/dbpedia-linkedgeodata-lake>
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "DBPedia" ;
      <http://purl.org/dc/terms/license>
              <https://github.com/dbpedia/dbpedia-links/blob/master/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from https://github.com/dbpedia/dbpedia-links" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://github.com/dbpedia/dbpedia-links> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_acceptance> ;
      lden:hasMetric <http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_metric> ;
      lden:hasReview <http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_review> ;
      lden:hasSource <http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_source> ;
      lden:hasTarget <http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_target> ;
      lden:outputFormat "alignment" .

<http://www.linklion.org/lden/dbpedia-linkedgeodata-lake_source>
      a       lden:SourceDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint <http://160.45.137.69:3030/dbpedia/query> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "{?a a dbpedia-owl:Lake}." ;
      lden:variable "a" .
