lden:bnb-dbpedia-novelists_spec_metric
      a       lden:Metric ;
      lden:expression "ADD(0.333333333333333333*Equality(a.bibliographic:has_created.dcterms:title, b\\dbpediaowl:author.dbpp:name[@lang='en']), 0.333333333333333333*trigrams(a.foaf:name, b.foaf:name), 0.333333333333333333*Equality(a.bibliographic:hasCreated.bibo:isbn10, b\\dbpediaowl:author.dbpp:isbn))" .

lden:bnb-dbpedia-novelists_spec_source
      a       lden:SourceDataset ;
      rdfs:label "bluk-bnb" ;
      lden:endPoint <http://bnb.data.bl.uk/sparql> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property foaf:name , (bibliographic:has_created dcterms:title) , (bibliographic:hasCreated bibo:isbn10) ;
      lden:restriction "?a a foaf:Person ; bibliographic:has_created ?work " ;
      lden:variable "?a" .

lden:bnb-dbpedia-novelists_spec_target
      a       lden:TargetDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint <http://dbpedia.org/sparql> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property foaf:name , (dbpedia-owl:author dbpp:isbn) , (dbpedia-owl:author <http://dbpedia.org/property/name[@lang='en']>) ;
      lden:restriction "?novel dbpediaowl:author ?b " ;
      lden:variable "?b" .

