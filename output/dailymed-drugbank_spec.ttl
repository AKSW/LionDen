lden:dailymed-drugbank_spec_source
      a       lden:SourceDataset ;
      rdfs:label "daily" ;
      lden:endPoint <Examples/GeneticEval/Datasets/dailymed-drugbank-ingredients/source2.csv> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:property <name> ;
      lden:restriction "" ;
      lden:variable "?x" .

lden:dailymed-drugbank_spec_acceptance
      a       lden:Acceptance ;
      lden:file <dailymed_drugbank_incredients_accepted.txt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.55"^^xsd:double .

lden:dailymed-drugbank_spec_metric
      a       lden:Metric ;
      lden:expression "AND(cosine(x.name,y.name)|0.146,AND(AND(trigrams(x.name,y.name)|0.49,jaccard(x.name,y.name)|0.65)|0.51,trigrams(x.name,y.name)|0.51)|0.51)" .

lden:dailymed-drugbank_spec_target
      a       lden:TargetDataset ;
      rdfs:label "drugbank" ;
      lden:endPoint <Examples/GeneticEval/Datasets/dailymed-drugbank-ingredients/target2.csv> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:property <name> ;
      lden:restriction "" ;
      lden:variable "?y" .

lden:dailymed-drugbank_spec_review
      a       lden:Review ;
      lden:file <dailymed_drugbank_incredients_reviewme.txt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.4"^^xsd:double .

lden:dailymed-drugbank_spec
      a       lden:LimesSpecs ;
      dcterms:created "2011" ;
      dcterms:creator "AKSW" ;
      dcterms:license <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      dcterms:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dcterms:publisher "AKSW" ;
      dcterms:source <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "SIMPLE" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:dailymed-drugbank_spec_acceptance ;
      lden:hasMetric lden:dailymed-drugbank_spec_metric ;
      lden:hasReview lden:dailymed-drugbank_spec_review ;
      lden:hasSource lden:dailymed-drugbank_spec_source ;
      lden:hasTarget lden:dailymed-drugbank_spec_target ;
      lden:outputFormat "N3" .
