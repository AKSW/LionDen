@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swpo:    <http://sw-portal.deri.org/ontologies/swportal#> .
@prefix lden:    <www.linklion.org/lden#> .
@prefix akt:     <http://www.aktors.org/ontology/portal#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix BibTeX:  <http://data.bibbase.org/ontology/#> .

<www.linklion.org/lden#/citeseer-dblp-author_spec_target>
      a       lden:TargetDataset ;
      rdfs:label "citeseer" ;
      lden:endPoint <http://citeseer.rkbexplorer.com/sparql/> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?b rdf:type akt:Person" ;
      lden:variable "b" .

<www.linklion.org/lden#/citeseer-dblp-author_spec_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/citeseer-dblp-author_spec_metric>
      a       lden:Metric ;
      lden:expression "max(jaro(b.akt:full-name,a.rdfs:label|0.90))" .

<www.linklion.org/lden#/citeseer-dblp-author_spec>
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "LATC" ;
      <http://purl.org/dc/terms/license>
              <https://github.com/LATC/sandbox/blob/master/linksets/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://github.com/LATC/sandbox/tree/master/linksets> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <www.linklion.org/lden#/citeseer-dblp-author_spec_acceptance> ;
      lden:hasMetric <www.linklion.org/lden#/citeseer-dblp-author_spec_metric> ;
      lden:hasReview <www.linklion.org/lden#/citeseer-dblp-author_spec_review> ;
      lden:hasSource <www.linklion.org/lden#/citeseer-dblp-author_spec_source> ;
      lden:hasTarget <www.linklion.org/lden#/citeseer-dblp-author_spec_target> ;
      lden:outputFormat "alignment" .

<www.linklion.org/lden#/citeseer-dblp-author_spec_source>
      a       lden:SourceDataset ;
      rdfs:label "bibbase" ;
      lden:endPoint <http://data.bibbase.org:2020/sparql> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?a rdf:type BibTeX:Author" ;
      lden:variable "a" .

<www.linklion.org/lden#/citeseer-dblp-author_spec_acceptance>
      a       lden:Acceptance ;
      lden:file <authorciteseerbibbase_links_verify.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.9"^^<http://www.w3.org/2001/XMLSchema#double> .
