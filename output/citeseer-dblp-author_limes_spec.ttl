@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lden:    <www.linklion.org/lden#> .
@prefix akt:     <http://www.aktors.org/ontology/portal#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix BibTeX:  <http://data.bibbase.org/ontology/#> .

lden:citeseer-dblp-author_spec_review
      a       lden:Review ;
      lden:file <authorciteseerbibbase_links_accepted.nt> ;
      lden:relation <> ;
      lden:threshold "0.9"^^<http://www.w3.org/2001/XMLSchema#double> .

lden:citeseer-dblp-author_spec_target
      a       lden:TargetDataset ;
      rdfs:label "citeseer" ;
      lden:endPoint <http://citeseer.rkbexplorer.com/sparql/> ;
      lden:pageSize "1000"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:property akt:full-name ;
      lden:restriction "?b rdf:type akt:Person" ;
      lden:variable "?b" .

lden:citeseer-dblp-author_spec_metric
      a       lden:Metric ;
      lden:expression "MAX(Jaro(a.rdfs:label, b.akt:full-name))" .

lden:citeseer-dblp-author_spec_acceptance
      a       lden:Acceptance ;
      lden:file <authorciteseerbibbase_links_accepted.nt> ;
      lden:relation <> ;
      lden:threshold "0.9"^^<http://www.w3.org/2001/XMLSchema#double> .

lden:citeseer-dblp-author_spec_source
      a       lden:SourceDataset ;
      rdfs:label "bibbase" ;
      lden:endPoint <http://data.bibbase.org:2020/sparql> ;
      lden:pageSize "1000"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:property rdfs:label ;
      lden:restriction "?a rdf:type BibTeX:Author" ;
      lden:variable "?a" .

lden:citeseer-dblp-author_spec
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "AKSW" ;
      <http://purl.org/dc/terms/license>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "Simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance lden:citeseer-dblp-author_spec_acceptance ;
      lden:hasMetric lden:citeseer-dblp-author_spec_metric ;
      lden:hasReview lden:citeseer-dblp-author_spec_review ;
      lden:hasSource lden:citeseer-dblp-author_spec_source ;
      lden:hasTarget lden:citeseer-dblp-author_spec_target ;
      lden:outputFormat "NT" .
