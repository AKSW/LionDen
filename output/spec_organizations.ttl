<www.linklion.org/lden#/spec_organizations_target>
      a       lden:TargetDataset ;
      rdfs:label "Dailymed" ;
      lden:endPoint <http://www4.wiwiss.fu-berlin.de/dailymed/sparql> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?b rdf:type dailymed:Organization" ;
      lden:variable "b" .

<www.linklion.org/lden#/spec_organizations_acceptance>
      a       lden:Acceptance ;
      lden:file <dailymed_dbpedia_organizations.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.98"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/spec_organizations_metric>
      a       lden:Metric ;
      lden:expression "max(jaro(b.rdfs:label,a.dbpedia-prop:wikiPageRedirects|0.95),jaroWinkler(b.rdfs:label,a.rdfs:label|0.95))" .

<www.linklion.org/lden#/spec_organizations>
      a       lden:LimesSpecs ;
      dc:created "2011" ;
      dc:creator "DBPedia" ;
      dc:license <https://github.com/dbpedia/dbpedia-links/blob/master/README> ;
      dc:provenance "Data are obtained from https://github.com/dbpedia/dbpedia-links" ;
      dc:publisher "AKSW" ;
      dc:source <https://github.com/dbpedia/dbpedia-links> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <www.linklion.org/lden#/spec_organizations_acceptance> ;
      lden:hasMetric <www.linklion.org/lden#/spec_organizations_metric> ;
      lden:hasReview <www.linklion.org/lden#/spec_organizations_review> ;
      lden:hasSource <www.linklion.org/lden#/spec_organizations_source> ;
      lden:hasTarget <www.linklion.org/lden#/spec_organizations_target> ;
      lden:outputFormat "ntriples" .

<www.linklion.org/lden#/spec_organizations_source>
      a       lden:SourceDataset ;
      rdfs:label "DBpedia" ;
      lden:endPoint <http://dbpedia.org/sparql> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?a rdf:type dbpedia-owl:Organisation" ;
      lden:variable "a" .

<www.linklion.org/lden#/spec_organizations_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .
