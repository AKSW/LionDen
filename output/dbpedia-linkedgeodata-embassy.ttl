@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix swpo:    <http://sw-portal.deri.org/ontologies/swportal#> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix lden:    <www.linklion.org/lden#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .
@prefix dbpprop:  <http://dbpedia.org/property/> .

<www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_source>
      a       lden:SourceDataset ;
      rdfs:label "geonames" ;
      lden:endPoint
              [] ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "{?a rdf:type lgdo:Embassy}" ;
      lden:variable "a" .

<www.linklion.org/lden#/dbpedia-linkedgeodata-embassy>
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "DBPedia" ;
      <http://purl.org/dc/terms/license>
              <https://github.com/dbpedia/dbpedia-links/blob/master/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from https://github.com/dbpedia/dbpedia-links" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://github.com/dbpedia/dbpedia-links> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_acceptance> ;
      lden:hasMetric <www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_metric> ;
      lden:hasReview <www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_review> ;
      lden:hasSource <www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_source> ;
      lden:hasTarget <www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_target> ;
      lden:outputFormat "ntriples" .

<www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_target>
      a       lden:TargetDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint
              [] ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "{?b rdf:type dbpedia-owl:Country}" ;
      lden:variable "b" .

<www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_acceptance>
      a       lden:Acceptance ;
      lden:file <dbpedia-linkedgeodata-mountain_verify.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.95"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/dbpedia-linkedgeodata-embassy_metric>
      a       lden:Metric ;
      lden:expression "average(wgs84(b.wgs84:lat/wgs84:long,a.wgs84:lat/wgs84:long|0.9),max(jaro(b.rdfs:label,a.rdfs:label|0.9)))" .
