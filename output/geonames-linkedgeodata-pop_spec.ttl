lden:geonames-linkedgeodata-pop_spec
      a       lden:LimesSpecs ;
      dcterms:created "2011" ;
      dcterms:creator "AKSW" ;
      dcterms:license <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      dcterms:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dcterms:publisher "AKSW" ;
      dcterms:source <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "Simple" ;
      lden:granularity "16"^^xsd:int ;
      lden:hasAcceptance lden:geonames-linkedgeodata-pop_spec_acceptance ;
      lden:hasMetric lden:geonames-linkedgeodata-pop_spec_metric ;
      lden:hasReview lden:geonames-linkedgeodata-pop_spec_review ;
      lden:hasSource lden:geonames-linkedgeodata-pop_spec_source ;
      lden:hasTarget lden:geonames-linkedgeodata-pop_spec_target ;
      lden:outputFormat "TAB" .

lden:geonames-linkedgeodata-pop_spec_source
      a       lden:SourceDataset ;
      rdfs:label "geonames" ;
      lden:endPoint <http://lgd.aksw.org:8900/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:property geonames:alternateName , geonames:population , geonames:name ;
      lden:restriction "" ;
      lden:variable "?x" .

lden:geonames-linkedgeodata-pop_spec_target
      a       lden:TargetDataset ;
      rdfs:label "lgd" ;
      lden:endPoint <http://linkedgeodata.org/sparql/> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property rdfs:label , lgdo:population ;
      lden:restriction "?y rdf:type lgdo:Place" ;
      lden:variable "?y" .

lden:geonames-linkedgeodata-pop_spec_review
      a       lden:Review ;
      lden:file <simpop_review.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^xsd:double .

lden:geonames-linkedgeodata-pop_spec_acceptance
      a       lden:Acceptance ;
      lden:file <places.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold 1.0E-5 .

lden:geonames-linkedgeodata-pop_spec_metric
      a       lden:Metric ;
      lden:expression "AND(euclidean(x.dbpedia-o:populationTotal,y.lgdo:population)|0.0001, OR(trigrams(x.geonames:alternateName, y.rdfs:label)|0.9, trigrams(x.geonames:name, y.rdfs:label)|0.9)|0.9)" .
