<www.linklion.org/lden#/spec>
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "DBPedia" ;
      <http://purl.org/dc/terms/license>
              <https://github.com/dbpedia/dbpedia-links/blob/master/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from https://github.com/dbpedia/dbpedia-links" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://github.com/dbpedia/dbpedia-links> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:hasAcceptance <www.linklion.org/lden#/spec_acceptance> ;
      lden:hasMetric <www.linklion.org/lden#/spec_metric> ;
      lden:hasReview <www.linklion.org/lden#/spec_review> ;
      lden:hasSource <www.linklion.org/lden#/spec_source> ;
      lden:hasTarget <www.linklion.org/lden#/spec_target> ;
      lden:outputFormat "ntriples" .

<www.linklion.org/lden#/spec_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/spec_source>
      a       lden:SourceDataset ;
      rdfs:label "source_a" ;
      lden:endPoint
              [] ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "?a rdf:type foaf:Person" ;
      lden:variable "a" .

<www.linklion.org/lden#/spec_acceptance>
      a       lden:Acceptance ;
      lden:file <links.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.9"^^<http://www.w3.org/2001/XMLSchema#double> .

<www.linklion.org/lden#/spec_target>
      a       lden:TargetDataset ;
      rdfs:label "DBpedia" ;
      lden:endPoint <http://sparql.sindice.com/sparql/> ;
      lden:pageSize "-1"^^<http://www.w3.org/2001/XMLSchema#int> ;
      lden:restriction "{?b rdf:type dbpedia-owl:Person} UNION {?b rdf:type yago:Person100007846} UNION {?b rdf:type foaf:Person}" ;
      lden:variable "b" .

<www.linklion.org/lden#/spec_metric>
      a       lden:Metric ;
      lden:expression "average(levenshtein(b.akt:full-name,a.foaf:name|0.8))" .
