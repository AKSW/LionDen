@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix meshr:   <http://bio2rdf.org/ns/mesh#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix lden:    <www.linklion.org/lden#> .
@prefix geonames:  <http://www.geonames.org/ontology#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix akt:     <http://www.aktors.org/ontology/portal#> .
@prefix linkedct:  <http://data.linkedct.org/resource/linkedct/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .
@prefix BibTeX:  <http://data.bibbase.org/ontology/#> .

lden:geonames-linkedgeodata-cliff_spec_metric
      a       lden:Metric ;
      lden:expression "ADD(0.5*MAX(Jaro(a.geonames:name, b.rdfs:label), Jaro(a.geonames:alternateName, b.rdfs:label)), 0.5*WGS84(a.wgs84:lat, b.wgs84:lat))" .

lden:geonames-linkedgeodata-cliff_spec_review
      a       lden:Review ;
      lden:file <1303320108589/cliff/links.nt> ;
      lden:relation <> ;
      lden:threshold "0.95"^^xsd:double .

lden:geonames-linkedgeodata-cliff_spec_source
      a       lden:SourceDataset ;
      rdfs:label "geonames" ;
      lden:endPoint <http://lgd.aksw.org:8900/sparql> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property wgs84:long , wgs84:lat , geonames:alternateName , geonames:name ;
      lden:restriction "?a geonames:featureCode geonames:CLF. ?a wgs84:lat ?lat. ?a wgs84:long ?long" ;
      lden:variable "?a" .

lden:geonames-linkedgeodata-cliff_spec
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "AKSW" ;
      <http://purl.org/dc/terms/license>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "Simple" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:geonames-linkedgeodata-cliff_spec_acceptance ;
      lden:hasMetric lden:geonames-linkedgeodata-cliff_spec_metric ;
      lden:hasReview lden:geonames-linkedgeodata-cliff_spec_review ;
      lden:hasSource lden:geonames-linkedgeodata-cliff_spec_source ;
      lden:hasTarget lden:geonames-linkedgeodata-cliff_spec_target ;
      lden:outputFormat "NT" .

lden:geonames-linkedgeodata-cliff_spec_acceptance
      a       lden:Acceptance ;
      lden:file <1303320108589/cliff/links.nt> ;
      lden:relation <> ;
      lden:threshold "0.95"^^xsd:double .

lden:geonames-linkedgeodata-cliff_spec_target
      a       lden:TargetDataset ;
      rdfs:label "linkedgeodata" ;
      lden:endPoint <http://linkedgeodata.org/sparql/> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property wgs84:long , wgs84:lat , rdfs:label ;
      lden:restriction "?b a lgdo:Cliff. ?b wgs84:lat ?lat. ?b wgs84:long ?long" ;
      lden:variable "?b" .
