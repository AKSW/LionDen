lden:dbpedia-linkedgeodata-island_spec
      a       lden:LimesSpecs ;
      dcterms:created "2011" ;
      dcterms:creator "AKSW" ;
      dcterms:license <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      dcterms:provenance "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      dcterms:publisher "AKSW" ;
      dcterms:source <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "Simple" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:dbpedia-linkedgeodata-island_spec_acceptance ;
      lden:hasMetric lden:dbpedia-linkedgeodata-island_spec_metric ;
      lden:hasReview lden:dbpedia-linkedgeodata-island_spec_review ;
      lden:hasSource lden:dbpedia-linkedgeodata-island_spec_source ;
      lden:hasTarget lden:dbpedia-linkedgeodata-island_spec_target ;
      lden:outputFormat "NT" .

lden:dbpedia-linkedgeodata-island_spec_metric
      a       lden:Metric ;
      lden:expression "ADD(0.5*MAX(Jaro(a.rdfs:label, b.rdfs:label)), 0.5*WGS84(a.wgs84:lat, b.wgs84:lat))" .

lden:dbpedia-linkedgeodata-island_spec_target
      a       lden:TargetDataset ;
      rdfs:label "linkedgeodata" ;
      lden:endPoint <http://linkedgeodata.org/sparql/> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property wgs84:long , wgs84:lat , rdfs:label ;
      lden:restriction "?b a lgdo:Island. ?b wgs84:lat ?lat. ?b wgs84:long ?long" ;
      lden:variable "?b" .

lden:dbpedia-linkedgeodata-island_spec_acceptance
      a       lden:Acceptance ;
      lden:file <1301310200652/island/links_accepted.nt> ;
      lden:relation <> ;
      lden:threshold "0.95"^^xsd:double .

lden:dbpedia-linkedgeodata-island_spec_source
      a       lden:SourceDataset ;
      rdfs:label "dbpedia" ;
      lden:endPoint <http://live.dbpedia.org/sparql/> ;
      lden:pageSize "1000"^^xsd:int ;
      lden:property wgs84:long , wgs84:lat , rdfs:label ;
      lden:restriction "?a a dbpedia-owl:Island. ?a wgs84:lat ?lat. ?a wgs84:long ?long" ;
      lden:variable "?a" .

lden:dbpedia-linkedgeodata-island_spec_review
      a       lden:Review ;
      lden:file <1301310200652/island/links_accepted.nt> ;
      lden:relation <> ;
      lden:threshold "0.95"^^xsd:double .
