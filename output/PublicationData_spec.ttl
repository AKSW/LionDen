@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix meshr:   <http://bio2rdf.org/ns/mesh#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix lden:    <www.linklion.org/lden#> .
@prefix geonames:  <http://www.geonames.org/ontology#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix akt:     <http://www.aktors.org/ontology/portal#> .
@prefix linkedct:  <http://data.linkedct.org/resource/linkedct/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .
@prefix BibTeX:  <http://data.bibbase.org/ontology/#> .

lden:PublicationData_spec_review
      a       lden:Review ;
      lden:file <pub_reviewme.txt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.5"^^xsd:double .

lden:PublicationData_spec_source
      a       lden:SourceDataset ;
      rdfs:label "dblp2" ;
      lden:endPoint <Examples/GeneticEval/Datasets/DBLP-ACM/DBLP2.csv> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:property <title> , <authors> , <venue> ;
      lden:restriction "" ;
      lden:variable "?x" .

lden:PublicationData_spec_metric
      a       lden:Metric ;
      lden:expression "AND(trigrams(x.title,y.title)|0.11,euclidean(x.year,y.year)|0.83)" .

lden:PublicationData_spec_target
      a       lden:TargetDataset ;
      rdfs:label "acm" ;
      lden:endPoint <Examples/GeneticEval/Datasets/DBLP-ACM/ACM.csv> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:property <title> , <authors> , <venue> ;
      lden:restriction "" ;
      lden:variable "?y" .

lden:PublicationData_spec
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "AKSW" ;
      <http://purl.org/dc/terms/license>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "SIMPLE" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:PublicationData_spec_acceptance ;
      lden:hasMetric lden:PublicationData_spec_metric ;
      lden:hasReview lden:PublicationData_spec_review ;
      lden:hasSource lden:PublicationData_spec_source ;
      lden:hasTarget lden:PublicationData_spec_target ;
      lden:outputFormat "N3" .

lden:PublicationData_spec_acceptance
      a       lden:Acceptance ;
      lden:file <pub_accepted.txt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.62"^^xsd:double .
