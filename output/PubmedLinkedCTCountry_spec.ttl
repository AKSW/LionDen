@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix meshr:   <http://bio2rdf.org/ns/mesh#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix lden:    <http://www.linklion.org/lden/> .
@prefix geonames:  <http://www.geonames.org/ontology#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix akt:     <http://www.aktors.org/ontology/portal#> .
@prefix linkedct:  <http://data.linkedct.org/resource/linkedct/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .
@prefix BibTeX:  <http://data.bibbase.org/ontology/#> .

lden:PubmedLinkedCTCountry_spec_target
      a       lden:TargetDataset ;
      rdfs:label "mesh" ;
      lden:endPoint <http://mesh.bio2rdf.org/sparql> ;
      lden:pageSize "5000"^^xsd:int ;
      lden:property dc:title ;
      lden:restriction "?y rdf:type meshr:Concept" ;
      lden:variable "?y" .

lden:PubmedLinkedCTCountry_spec
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "AKSW" ;
      <http://purl.org/dc/terms/license>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://www.informatik.uni-leipzig.de/svn/an/devan/LIMES> ;
      <http://sw-portal.deri.org/ontologies/swportal#Tool>
              "limes" ;
      lden:executionPlan "Simple" ;
      lden:granularity "1"^^xsd:int ;
      lden:hasAcceptance lden:PubmedLinkedCTCountry_spec_acceptance ;
      lden:hasMetric lden:PubmedLinkedCTCountry_spec_metric ;
      lden:hasReview lden:PubmedLinkedCTCountry_spec_review ;
      lden:hasSource lden:PubmedLinkedCTCountry_spec_source ;
      lden:hasTarget lden:PubmedLinkedCTCountry_spec_target ;
      lden:outputFormat "TAB" .

lden:PubmedLinkedCTCountry_spec_acceptance
      a       lden:Acceptance ;
      lden:file <countries_accepted.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.9"^^xsd:double .

lden:PubmedLinkedCTCountry_spec_metric
      a       lden:Metric ;
      lden:expression "levenshtein(x.linkedct:facility_address_country, y.dc:title)" .

lden:PubmedLinkedCTCountry_spec_source
      a       lden:SourceDataset ;
      rdfs:label "linkedct" ;
      lden:endPoint <http://data.linkedct.org/sparql> ;
      lden:pageSize "5000"^^xsd:int ;
      lden:property linkedct:facility_address_country ;
      lden:restriction "?x rdf:type linkedct:location" ;
      lden:variable "?x" .

lden:PubmedLinkedCTCountry_spec_review
      a       lden:Review ;
      lden:file <countries_reviewme.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.8"^^xsd:double .
