@prefix lgdp:    <http://linkedgeodata.org/property/> .
@prefix lgdo:    <http://linkedgeodata.org/ontology/> .
@prefix swpo:    <http://sw-portal.deri.org/ontologies/swportal#> .
@prefix dbpedia-owl:  <http://dbpedia.org/ontology/> .
@prefix geo:     <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix lden:    <http://www.linklion.org/lden/> .
@prefix dbpedia-prop:  <http://dbpedia.org/property/> .
@prefix geonames:  <http://www.geonames.org/ontology#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix umbel-sc:  <http://umbel.org/umbel/sc/> .
@prefix akt:     <http://www.aktors.org/ontology/portal#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix dbpedia:  <http://dbpedia.org/ontology/> .
@prefix wgs84:   <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix linkedgeodata:  <http://linkedgeodata.org/triplify/> .
@prefix BibTeX:  <http://data.bibbase.org/ontology/#> .

<http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_acceptance>
      a       lden:Acceptance ;
      lden:file <1303320108589/cliff/verify.nt> ;
      lden:relation owl:sameAs ;
      lden:threshold "0.95"^^xsd:double .

<http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_target>
      a       lden:TargetDataset ;
      rdfs:label "linkedgeodata" ;
      lden:endPoint <http://linkedgeodata.org/sparql/> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?b a lgdo:Cliff.  ?b wgs84:lat ?lat. ?b wgs84:long ?long." ;
      lden:variable "b" .

<http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_metric>
      a       lden:Metric ;
      lden:expression "average(wgs84(b.wgs84:lat/wgs84:long,a.wgs84:lat/wgs84:long|0.9),max(jaro(b.rdfs:label,a.geonames:name|0.9),jaro(b.rdfs:label,a.geonames:alternateName|0.9)))" .

<http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec>
      a       lden:LimesSpecs ;
      <http://purl.org/dc/terms/created>
              "2011" ;
      <http://purl.org/dc/terms/creator>
              "LATC" ;
      <http://purl.org/dc/terms/license>
              <https://github.com/LATC/sandbox/blob/master/linksets/README> ;
      <http://purl.org/dc/terms/provenance>
              "Data are obtained from http://aksw.org/Projects/LATC.html" ;
      <http://purl.org/dc/terms/publisher>
              "AKSW" ;
      <http://purl.org/dc/terms/source>
              <https://github.com/LATC/sandbox/tree/master/linksets> ;
      swpo:Tool "silk" ;
      lden:executionPlan "simple" ;
      lden:granularity "2"^^xsd:int ;
      lden:hasAcceptance <http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_acceptance> ;
      lden:hasMetric <http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_metric> ;
      lden:hasReview <http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_review> ;
      lden:hasSource <http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_source> ;
      lden:hasTarget <http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_target> ;
      lden:outputFormat "ntriples" .

<http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_review>
      a       lden:Review ;
      lden:file
              [] ;
      lden:relation owl:sameAs ;
      lden:threshold "0.0"^^xsd:double .

<http://www.linklion.org/lden/geonames-linkedgeodata-cliff_spec_source>
      a       lden:SourceDataset ;
      rdfs:label "geonames" ;
      lden:endPoint <http://lgd.aksw.org:8900/sparql> ;
      lden:pageSize "-1"^^xsd:int ;
      lden:restriction "?a geonames:featureCode geonames:CLF.  ?a wgs84:lat ?lat. ?a wgs84:long ?long." ;
      lden:variable "a" .
